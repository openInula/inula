{
  "name": "@openinula/next-shared",
  "version": "0.0.2",
  "description": "Inula Next Shared",
  "keywords": [
    "Inula-Next"
  ],
  "license": "MIT",
  "files": [
    "src"
  ],
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsup --sourcemap"
  },
  "devDependencies": {
    "typescript": "^5.3.2",
    "tsup": "^8.2.4"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "clean": true,
    "dts": true,
    "sourceMap": true
  }
}
