{
  "name": "babel-preset-inula-next",
  "version": "0.0.1",
  "author": {
    "name": "IanDx",
    "email": "iandxssxx@gmail.com"
  },
  "keywords": [
    "dlight.js",
    "babel-preset"
  ],
  "license": "MIT",
  "files": [
