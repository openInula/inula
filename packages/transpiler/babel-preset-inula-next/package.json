    "@babel/plugin-syntax-typescript": "^7.23.3",
    "@inula/reactivity-parser": "workspace:*",
    "@inula/view-generator": "workspace:*",
    "@inula/view-parser": "workspace:*",
    "@inula/class-transformer": "workspace:*",
    "jsx-view-parser": "workspace:*",
    "minimatch": "^9.0.3",
    "vitest": "^1.4.0"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
