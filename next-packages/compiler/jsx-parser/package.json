{
  "name": "@openinula/jsx-view-parser",
  "version": "0.0.4",
  "description": "Inula jsx parser",
  "keywords": [
    "inula"
  ],
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsup --sourcemap",
    "test": "vitest --ui"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@types/babel__core": "^7.20.5",
    "@vitest/ui": "^1.2.1",
    "tsup": "^6.7.0",
    "typescript": "^5.3.2",
    "@babel/plugin-syntax-jsx": "7.23.3",
    "@openinula/error-handler": "workspace:*",
    "vitest": "^1.2.1"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "clean": true,
    "dts": true,
    "sourceMap": true
  },
  "publishConfig": {
    "access": "public"
  }
}
