{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "test": {
      "dependsOn": [
        "^test"
      ],
      "outputs": []
    },
    "typecheck": {
      "dependsOn": [
        "^typecheck"
      ],
      "outputs": []
    },
    "inula-router#build": {
      "dependsOn": [
        "openinula#build"
      ]
    },
    "inula-intl#build": {
      "dependsOn": [
        "openinula#build"
      ]
    }
  }
}